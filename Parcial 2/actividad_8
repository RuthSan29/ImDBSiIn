Perdon por la tardanza profeeee

ejercicio 1

CREATE TABLE Ventasv2 (
    A単o INT NOT NULL,
    Cantidad INT NOT NULL
);

INSERT INTO Ventasv2 VALUES
(2025, 352645),
(2024, 165565),
(2024, 254654),
(2023, 159521),
(2023, 251696),
(2023, 111894);

SELECT 
SUM(CASE WHEN A単o = 2025 THEN Cantidad END) AS '2025',
SUM(CASE WHEN A単o = 2024 THEN Cantidad END) AS '2024',
SUM(CASE WHEN A単o = 2023 THEN Cantidad END) AS '2023'
FROM Ventasv2;

