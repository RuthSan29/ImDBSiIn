ejercicio 2

CREATE TABLE muestra (
    valor INTEGER
);  

INSERT INTO muestra VALUES
 (1),(1),(2),(3),(3),(4);

Select 
    DISTINCT(valor)
From muestra;


-------------------------------------------------------------------------------------------------------------
 ejercicio 3

CREATE TABLE Huecos (
    Fila INT,
    Aplicacion VARCHAR(20),
    Estado VARCHAR(20)
);

INSERT INTO Huecos VALUES
(1, 'Web', 'Aprobado'),
(2, NULL, 'Fallo'),
(3, NULL, 'Fallo'),
(4, NULL, 'Fallo'),
(5, 'App', 'Aprobado'),
(6, NULL, 'Fallo'),
(7, NULL, 'Fallo'),
(8, NULL, 'Aprobado'),
(9, NULL, 'Aprobado'),
(10, 'RESTAPI', 'Fallo'),
(11, NULL, 'Fallo'),
(12, NULL, 'Fallo');

WITH Relleno AS (
  SELECT 
      Fila,
      Estado,
      Aplicacion,
      MAX(Aplicacion) OVER (ORDER BY Fila ROWS BETWEEN UNBOUNDED PRECEDING AND CURRENT ROW) AS Aplicacion_llenada
  FROM Huecos
)
SELECT 
    Fila, 
    Aplicacion_llenada AS Aplicacion, 
    Estado
FROM Relleno
ORDER BY Fila;
